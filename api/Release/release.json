[
    {
        "release": [
            {
                "method": "getRelease",
                "description": "Get a release of economic data.",
                "request-type": "GET",
                "parameters": {
                    "release_id": {
                        "required": true,
                        "type": "integer",
                        "default": "none",
                        "description": "The id for a release."
                    },
                    "realtime_start": {
                        "required": false,
                        "type": "date",
                        "default": "Today's Date",
                        "description": "The start of the real-time period."
                    },
                    "realtime_end": {
                        "required": false,
                        "type": "date",
                        "default": "9999-12-31 (latest available)",
                        "description": "The end of the real-time period."
                    }
                }
            }
        ]
    }
]