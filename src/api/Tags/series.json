[
    {
        "tags/series": [
            {
                "method": "getAllSeriesMatchingTags",
                "description": "Get the series matching all tags in the tag_names parameter and no tags in the exclude_tag_names parameter.",
                "request-type": "GET",
                "parameters": {
                    "tag_names": {
                        "required": true,
                        "type": "string",
                        "default": "none",
                        "description": "A semicolon delimited list of tag names that series match all of."
                    },
                    "realtime_start": {
                        "required": false,
                        "type": "date",
                        "default": "Today's Date",
                        "description": "The start of the real-time period."
                    },
                    "realtime_end": {
                        "required": false,
                        "type": "date",
                        "default": "Today's Date",
                        "description": "The end of the real-time period."
                    },
                    "limit": {
                        "required": false,
                        "type": "integer",
                        "default": 1000,
                        "description": "The maximum number of results to return",
                        "restrictions": "Must be an integer between 1 and 1000"
                    },
                    "offset": {
                        "required": false,
                        "type": "integer",
                        "default": 0,
                        "description": "A non negative integer",
                        "restrictions": "A non negative integer"
                    },
                    "order_by": {
                        "required": false,
                        "type": "string",
                        "default": "series_id",
                        "description": "Order results by values of the specified attribute.",
                        "restrictions": "One of the following strings: 'series_id', 'title', 'units', 'frequency', 'seasonal_adjustment', 'realtime_start', 'realtime_end', 'last_updated', 'observation_start', 'observation_end', 'popularity'."
                    },
                    "sort_order": {
                        "required": false,
                        "type": "string",
                        "default": "asc",
                        "description": "Sort results is ascending or descending order for attribute values specified by order_by.",
                        "restrictions": "One of the following strings: 'asc', 'desc'."
                    },
                    "exclude_tag_names": {
                        "required": false,
                        "type": "string",
                        "default": "none",
                        "description": "A semicolon delimited list of tag names that series match none of.",
                        "restrictions": "Parameter exclude_tag_names requires that parameter tag_names also be set to limit the number of matching series."
                    }
                }
            }
        ]
    }
]
