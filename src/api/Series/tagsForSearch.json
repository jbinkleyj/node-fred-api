[
    {
        "series/search/tags": [
            {
                "method": "getTagsForSeriesSearch",
                "description": "Get the FRED tags for a series search. Optionally, filter results by tag name, tag group, or tag search.",
                "request-type": "GET",
                "parameters": {
                    "series_id": {
                        "required": true,
                        "type": "integer",
                        "default": 0,
                        "description": "The id for a series."
                    },
                    "series_search_text": {
                        "required": true,
                        "type": "string",
                        "default": "none",
                        "description": "The words to match against economic data series."
                    },
                    "realtime_start": {
                        "required": false,
                        "type": "date",
                        "default": "Today's Date",
                        "description": "The start of the real-time period."
                    },
                    "realtime_end": {
                        "required": false,
                        "type": "date",
                        "default": "9999-12-31 (latest available)",
                        "description": "The end of the real-time period."
                    },
                    "limit": {
                        "required": false,
                        "type": "integer",
                        "default": 1000,
                        "description": "The maximum number of results to return",
                        "restrictions": "Must be an integer between 1 and 1000"
                    },
                    "offset": {
                        "required": false,
                        "type": "integer",
                        "default": 0,
                        "description": "A non negative integer",
                        "restrictions": "A non negative integer"
                    },
                    "order_by": {
                        "required": false,
                        "type": "string",
                        "default": "series_count",
                        "description": "Order results by values of the specified attribute.",
                        "restrictions": "One of the following strings: 'series_count', 'popularity', 'created', 'name', 'group_id'"
                        
                    },
                    "sort_order": {
                        "required": false,
                        "type": "string",
                        "default": "asc",
                        "description": "Sort results is ascending or descending order for attribute values specified by order_by.",
                        "restrictions": "One of the following strings: 'asc', 'desc'."
                    },
                    "tag_names": {
                        "required": false,
                        "type": "string",
                        "default": "none",
                        "description": "A semicolon delimited list of tag names that series match all of."
                    },
                    "tag_group_id": {
                        "required": false,
                        "type": "string",
                        "default": "none",
                        "description": "A tag group id to filter tags by type.",
                        "restrictions": "One of the following: 'freq', 'gen', 'geo', 'geot', 'rls', 'seas', 'src'."
                    },
                    "tag_search_text": {
                        "required": false,
                        "type": "string",
                        "default": "none",
                        "description": "The words to find matching tags with."
                    }
                }
            }
        ]
    }
]
