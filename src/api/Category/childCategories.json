[
    {
        "category/children": [
            {
                "method": "getChildCategories",
                "description": "Gets the child categories for a specified parent category.",
                "request-type": "GET",
                "parameters": {
                    "category_id": {
                        "required": true,
                        "type": "integer",
                        "default": 0,
                        "description": "The id for a category."
                    },
                    "realtime_start": {
                        "required": false,
                        "type": "date",
                        "default": "Today's Date",
                        "description": "The start of the real-time period."
                    },
                    "realtime_end": {
                        "required": false,
                        "type": "date",
                        "default": "9999-12-31 (latest available)",
                        "description": "The end of the real-time period."
                    }
                }
            }
        ]
    }
]
